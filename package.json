{
  "name": "baris-sencan-website",
  "version": "1.0.0",
  "private": true,
  "engine": {
    "node": "14.15"
  },
  "scripts": {
    "postinstall": "husky install",
    "dev": "nuxt",
    "build": "nuxt generate",
    "lint": "npm run test:lint -- --fix",
    "test": "run-p --print-label test:*",
    "test:lint": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore",
    "test:type": "tsc --noEmit",
    "deploy": "touch ./dist/.nojekyll && echo 'barissencan.com' > ./dist/CNAME && gh-pages -d dist -t true"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.3.5",
    "@stripe/stripe-js": "^1.17.0",
    "axios": "^0.21.1",
    "core-js": "^3.18.3",
    "date-fns": "^2.23.0",
    "firebase": "^8.10.0",
    "lodash": "^4.17.21",
    "nuxt": "^2.15.8",
    "prismjs": "^1.24.1",
    "validator": "^13.6.0",
    "vuetify-markdown-editor": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^13.1.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/vuetify": "^1.12.1",
    "@types/date-fns": "^2.6.0",
    "@types/lodash": "^4.14.172",
    "@types/parseurl": "^1.3.1",
    "@types/prismjs": "^1.16.6",
    "@types/validator": "^13.6.3",
    "babel-eslint": "^10.1.0",
    "commitlint": "^13.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-markdown": "^2.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.16.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5"
  }
}
